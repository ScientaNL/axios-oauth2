{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": true,
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["DOM","esnext"]
	},
	"exclude": [
		"./node_modules",
		"./dist",
		"./tests,",
		"./coverage,"
	]
}
